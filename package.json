{
	"name": "bare-cms",
	"version": "0.0.1",
	"description": "Bare CMS",
	"main": "index.js",
	"scripts": {
		"test": "test",
		"start": "concurrently \"npm run start:api\" \"npm run start:app\"",
		"start:api": "nodemon server.js --prefix api",
		"start:app": "npm run serve --prefix app",
		"start:lambda": "netlify-lambda serve src/lambda",
		"init": "npm run init:api && npm run init:app",
		"init:app": "npm install --prefix app",
		"init:api": "npm install",
		"build": "npm run build:lambda && npm run build:app",
		"build:lambda": "netlify-lambda build api",
		"build:app": "npm run build --prefix app"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/silvandiepen/bare-cms.git"
	},
	"keywords": [
		"Bare",
		"CMS"
	],
	"author": "Sil van Diepen",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/silvandiepen/bare-cms/issues"
	},
	"homepage": "https://github.com/silvandiepen/bare-cms#readme",
	"devDependencies": {
		"@henris/custom": "0.0.2",
		"guyn": "0.0.13",
		"henris": "^1.2.18",
		"nodemon": "^2.0.2"
	},
	"dependencies": {
		"concurrently": "^5.0.2",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"http-proxy-middleware": "^0.20.0",
		"mongodb-client-encryption": "^1.0.1",
		"mongoose": "^5.8.7",
		"netlify-lambda": "^1.6.3"
	}
}
